{
  "config": {
    "connector.class": "com.aliyun.oss.connect.kafka.OSSSinkConnector",
    "partition.duration.ms": "10000",
    "tasks.max": "1",
    "timezone": "Asia/Jakarta",
    "rotate.interval.ms": "1000",
    "locale": "US",
    "name": "oss-sink",
    "storage.class": "com.aliyun.oss.connect.kafka.storage.OSSStorage",
    "oss.bucket": "data-lake-",
    "flush.size": "5",
    "timestamp.extractor": "RecordField",
    "timestamp.field": "timestamp",
    "path.format": "'dt'=YYYY-MM-dd",
    "format.class": "com.aliyun.oss.connect.kafka.format.parquet.ParquetJsonFormat",
    "partitioner.class": "io.confluent.connect.storage.partitioner.TimeBasedPartitioner",
    "topics": "events21",
    "oss.parquet.protobuf.schema.class": "events21,com.aliyun.oss.connect.kafka.format.parquet.Parquet$TestMessage",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false"
  },
  "name": "oss-sink"
}
